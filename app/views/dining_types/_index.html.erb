 <% @dining_types.each do |dining_type| %>
 <tr>
  <td class="col-md-1"><%= image_tag(dining_type.icon.url(:square), :class => "img_circle") %></td>
  <td class="col-md-9"><%= dining_type.name %></td>
  <td class="col-md-1"><%= link_to edit_dining_type_path(dining_type), remote: true do %>
    <i class="fa fa-pencil" style="color:white"></i><% end %>
  </td>
  <td class="col-md-1"><%= link_to dining_type, method: :delete, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o" style="color:white"></i><% end %>
  </td>
</tr>
<% end %>