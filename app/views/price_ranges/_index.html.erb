 <% @price_ranges.each do |price_range| %>
 <tr>
  <td class="col-md-9"><%= price_range.name %></td>
  <td class="col-md-1"><%= link_to edit_price_range_path(price_range), remote: true do %>
    <i class="fa fa-pencil" style="color:brown"></i><% end %>
  </td>
  <td class="col-md-1"><%= link_to price_range, method: :delete, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o" style="color:brown"></i><% end %>
  </td>
</tr>
<% end %>