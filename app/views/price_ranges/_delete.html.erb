<div class="modal-dialog">
  <div class="modal-content">
  <div class="modal-header">
    <h3>Delete Price Range</h3>
  </div>
  <div class="modal-body">
  <ul class="errors"></ul>
  Are you sure you wish to delete <b><%= @price_range.price %></b>?
  </div>
  <div class="modal-footer">
    <%= link_to "Delete", price_range_path(@price_range), method: :delete, remote: true, class: "btn btn-danger" %>
    <%= link_to "Cancel", "#", class: "btn btn-default", data: { dismiss: "modal" } %>
  </div>
  </div>
</div>