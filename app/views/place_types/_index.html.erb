 <% @place_types.each do |place_type| %>
 <tr>
  <td class="col-md-1"><%= image_tag(place_type.icon.url(:square), :class => "img_circle") %></td>
  <td class="col-md-9"><%= place_type.name %></td>
  <td class="col-md-1"><%= link_to edit_place_type_path(place_type), remote: true do %>
    <i class="fa fa-pencil" style="color:white"></i><% end %>
  </td>
  <td class="col-md-1"><%= link_to place_type, method: :delete, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o" style="color:white"></i><% end %>
  </td>
</tr>
<% end %>