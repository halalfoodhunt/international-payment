<% @food_types.each do |food_type| %>
      <tr>
        <td class="col-md-1"><%= image_tag(food_type.icon.url(:square), :class => "img_circle") %></td>
        <td class="col-md-9"><%= food_type.name %></td>
        <td class="col-md-1"><%= link_to edit_food_type_path(food_type), remote: true do %>
          <i class="fa fa-pencil" style="color:white"></i><% end %>
        </td>
        <td class="col-md-1"><%= link_to food_type, method: :delete, data: { confirm: 'Are you sure?' } do %>
        <i class="fa fa-trash-o" style="color:white"></i><% end %>
        </td>
      </tr>
<% end %>