 <% @cuisine_types.each do |cuisine_type| %>
 <tr>
  <td class="col-md-1"><%= image_tag(cuisine_type.icon.url(:square), :class => "img_circle") %></td>
  <td class="col-md-9"><%= cuisine_type.name %></td>
  <td class="col-md-1"><%= link_to edit_cuisine_type_path(cuisine_type), remote: true do %>
    <i class="fa fa-pencil" style="color:brown"></i><% end %>
  </td>
  <td class="col-md-1"><%= link_to cuisine_type, method: :delete, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o" style="color:brown"></i><% end %>
  </td>
</tr>
<% end %>