<% @catering_types.each do |catering_type| %>
<tr>
  <td class="col-md-1"><%= image_tag(catering_type.icon.url(:square), :class => "img_circle") %></td>
  <td class="col-md-9"><%= catering_type.name %></td>
  <td class="col-md-1"><%= link_to edit_catering_type_path(catering_type), remote: true do %>
    <i class="fa fa-pencil" style="color:white"></i><% end %>
  </td>
  <td class="col-md-1"><%= link_to catering_type, method: :delete, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o" style="color:white"></i><% end %>
  </td>
</tr>
<% end %>