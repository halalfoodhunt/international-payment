 <% @establishment_types.each do |establishment_type| %>
 <tr>
  <td class="col-md-1"><%= image_tag(establishment_type.icon.url(:square), :class => "img_circle") %></td>
  <td class="col-md-9"><%= establishment_type.name %></td>
  <td class="col-md-1"><%= link_to edit_establishment_type_path(establishment_type), remote: true do %>
    <i class="fa fa-pencil" style="color:brown"></i><% end %>
  </td>
  <td class="col-md-1"><%= link_to establishment_type, method: :delete, data: { confirm: 'Are you sure?' } do %>
    <i class="fa fa-trash-o" style="color:brown"></i><% end %>
  </td>
</tr>
<% end %>